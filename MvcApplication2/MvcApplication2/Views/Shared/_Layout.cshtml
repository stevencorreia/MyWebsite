<body>
    <header>
        <div class="content-wrapper">
            <div class="float-left">
                <p class="site-title">
                </p>
            </div>
            <div class="float-right">
                @*<section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>*@
                <nav>
                    <ul id="menu">
                        <li>@Html.ActionLink("Home", "Index", "Personal")</li>
                        <li>@Html.ActionLink("About Me", "About", "Personal")</li>
                        @*<li>@Html.ActionLink("Family Tree", "OnDemand", "Treeview")</li>*@
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    <div id="body">
        @RenderSection("featured", required: false)
        <section class="content-wrapper main-content clear-fix">
            @RenderBody()
        </section>
    </div>
    <footer>
        <div class="content-wrapper">
            <div class="float-left">
                <p><b>Contact:</b> rectofine@gmail.com  (806)317-2514 </p>
            </div>
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
